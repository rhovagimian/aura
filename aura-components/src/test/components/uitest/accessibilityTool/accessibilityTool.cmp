          <h6>6thHeader</h6>
          <h5>5thHeader</h5>
          <h4>4thHeader</h4>
          <h3>3rdHeader</h3>
          <h2>2ndHeader</h2>
          <h1>1stHeader</h1>
    </aura:if>
    <aura:if isTrue="{!v.caseToRender == 'headersWrongOrderWrapAround'}">
          <h1>1stHeader</h1>
          <h3>3rdHeader</h3>
          <h5>5thHeader</h5>
          <h6>6thHeader</h6>
          <h4>4thHeader</h4>
    </aura:if>
    <!--Test that will make sure that the accessibility tool can handle empty tags that do not have any attributes-->
    <aura:if isTrue="{!v.caseToRender == 'tagsWithoutAttributes'}">
        <input/>
        <a/>
        <img/>
        <button/>
        <iframe/>
    </aura:if>
</aura:component>
